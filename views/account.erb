<div class="container">
    <form action="account" method="post">


<p><table></p>
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Street</th>
                <th>City</th>
                <th>State</th>
                <th>Zip</th>
                <th>Phone Number</th>
                <th>Email Address</th>
            </tr>
        </thead>
        <tbody>

            <%accountinfo.each do |info| %>

            <tr>
                <td><%=info["first"] %></td>
                <td><%=info["last"] %></td>
                <td><%=info["street"] %></td>
                <td><%=info["city"] %></td>
                <td><%=info["state"] %></td>
                <td><%=info["zip"] %></td>
                <td><%=info["phonenumber"] %></td>
                <td><%=info["email"] %></td>
            </tr>


            <%end%>
        </tbody>
    </table>








    <p>
        <a class="pure-button btn-mm" href="/p_info">Continue Application</a>
        <button type="submit" class="pure-button btn-mm">Logout</button>
    </p>
</div>
